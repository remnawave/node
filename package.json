    "@types/semver": "^7.7.0",
    "@typescript-eslint/eslint-plugin": "8.40.0",
    "@typescript-eslint/parser": "8.40.0",
    "eslint": "9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-paths": "^1.1.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-prettier": "^5.5.4",
    "knip": "^5.62.0",
    "prettier": "^3.6.2",
