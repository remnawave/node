    "nest-winston": "^1.10.2",
    "nestjs-zod": "4.3.1",
    "node-supervisord": "^1.0.6-rc.2",
    "object-hash": "^3.0.0",
    "p-map": "^7.0.3",
    "p-retry": "^6.2.1",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "pkg-types": "^2.3.0",
    "pretty-bytes": "^6.1.1",
    "reflect-metadata": "0.2.2",
